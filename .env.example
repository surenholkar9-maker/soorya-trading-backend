# Soorya Trading Backend - Environment Variables
# Copy this file to .env and fill in with your actual credentials

# Upstox API Configuration
# Get these from https://developer.upstox.com
UPSTOX_API_KEY=your_api_key_here
UPSTOX_API_SECRET=your_api_secret_here

# Redirect URI for OAuth
# This should match your frontend URL + /callback
REDIRECT_URI=https://surenholkar9-maker.github.io/soorya-trading/callback

# Node Environment
NODE_ENV=production

# Port for local development
PORT=3000
